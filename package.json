{
  "name": "me",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --root sites/main-site",
    "dev:main": "astro dev --root sites/main-site",
    "dev:diy": "astro dev --root sites/diy-site",
    "build": "astro build --root sites/main-site",
    "build:main": "astro build --root sites/main-site",
    "build:diy": "astro build --root sites/diy-site",
    "preview": "astro preview --root sites/main-site",
    "preview:main": "astro preview --root sites/main-site",
    "preview:diy": "astro preview --root sites/diy-site",
    "astro": "astro",
    "env:local": "node sites/main-site/switch-env.js local",
    "env:production": "node sites/main-site/switch-env.js production",
    "env:status": "echo 'Main site:' && grep STRAPI_URL sites/main-site/.env 2>/dev/null && echo 'DIY site:' && grep STRAPI_URL sites/diy-site/.env 2>/dev/null || echo 'Some .env files missing'",
    "env:main:local": "node sites/main-site/switch-env.js local",
    "env:main:production": "node sites/main-site/switch-env.js production"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.1",
    "astro": "^5.12.3",
    "open-props": "^1.7.12",
    "tweakpane": "^4.0.5"
  }
}
