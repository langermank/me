{
  "kind": "collectionType",
  "collectionName": "case_studies",
  "info": {
    "singularName": "case-study",
    "pluralName": "case-studies",
    "displayName": "Case study"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "text",
      "required": true
    },
    "hasMore": {
      "type": "boolean",
      "default": false
    },
    "type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::post-type.post-type",
      "required": true
    },
    "externalUrl": {
      "type": "string"
    },
    "darkThumbnail": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "content": {
      "type": "richtext"
    }
  }
}
